# StarRocks Database Configuration
SR_HOST=localhost
SR_PORT=9030
SR_USER=root
SR_PASSWORD=
SR_DATABASE=information_schema

# HTTP Server Configuration
PORT=3000

# Security - API Key for authentication (optional but recommended)
# Generate a secure random key: openssl rand -hex 32
API_KEY=your-secret-api-key-here

# CORS - Comma-separated list of allowed origins
# Use '*' to allow all origins (not recommended for production)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# LLM API Keys (for intelligent analysis)
DEEPSEEK_API_KEY=
OPENAI_API_KEY=
GEMINI_API_KEY=

# Remote Agent Configuration (for multi-tenant mode)
# Path to tenants configuration file
TENANTS_CONFIG=./tenants-config.json

# Local Agent Configuration (when running as agent)
AGENT_PORT=8080
AGENT_TOKEN=
